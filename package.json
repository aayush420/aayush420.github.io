{
  "name": "aayush420.github.io",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "deploy": "vite build && echo '' > dist/.gitignore && git add dist && git commit -m 'Prod build' && git push && git subtree push --prefix dist origin gh-pages && echo '*' > dist/.gitignore"
  },
  "dependencies": {
    "vue": "latest",
    "vue-router": "^4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "autoprefixer": "latest",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.18.0",
    "postcss": "latest",
    "prettier": "^2.4.1",
    "sass": "latest",
    "tailwindcss": "latest",
    "vite": "latest"
  }
}
